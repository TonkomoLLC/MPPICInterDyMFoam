FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      createBafflesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

internalFacesOnly true;

baffles
{
    AMI
    {
        type        faceZone;
        zoneName    AMI;

        patchPairs
        {
            type            cyclicAMI;
            transform       noOrdering;
        }
    }
}


