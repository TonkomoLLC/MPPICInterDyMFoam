FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0.225";
    object      rAU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [-1 3 1 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    vessel
    {
        type            calculated;
        value           uniform 1;
    }
    inlet
    {
        type            calculated;
        value           uniform 1;
    }
    outlet
    {
        type            calculated;
        value           uniform 1;
    }
    shaftRotating
    {
        type            calculated;
        value           uniform 1;
    }
    shaftStatic
    {
        type            calculated;
        value           uniform 1;
    }
    AMI1
    {
        type            cyclicAMI;
        value           uniform 1;
    }
    AMI2
    {
        type            cyclicAMI;
        value           uniform 1;
    }
}


